# Server Configuration
PORT=8787
NODE_ENV=development
SESSION_SECRET=replace-with-secure-random-string

# Database & Storage (Supabase)
# Frontend (Vite) needs VITE_ prefix
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=replace-with-your-anon-key

# Backend needs non-prefixed versions for server-side operations
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=replace-with-service-role-key

# AI Services
GEMINI_API_KEY=replace-with-gemini-key

# ElevenLabs Integration
ELEVENLABS_API_KEY=replace-with-elevenlabs-key
ELEVENLABS_TOOL_SECRET=replace-with-tool-secret
ELEVENLABS_WEBHOOK_SECRET=replace-with-webhook-secret

# ElevenLabs Agent IDs (for ConvAI Widget)
VITE_ELEVENLABS_AGENT_ID_LEVI=replace-with-levi-agent-id
VITE_ELEVENLABS_AGENT_ID_VICTORIA=replace-with-victoria-agent-id

# Redis (Caching & Queues) - Required for rate limiting and job queues
REDIS_URL=redis://localhost:6379

# Remote Services (Optional - has fallbacks)
BAZI_ENGINE_URL=https://baziengine-v2.fly.dev

# Demo Mode (Optional - defaults to true)
VITE_DEMO_MODE=false

# Google Maps API (Optional - for location autocomplete)
VITE_GOOGLE_MAPS_API_KEY=replace-with-google-maps-key
