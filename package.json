{
  "name": "mvp-onboarding-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:web\"",
    "dev:web": "vite",
    "dev:server": "tsx server/server.ts",
    "build": "vite build && tsc --project tsconfig.server.json",
    "build:frontend": "vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "preview": "vite preview",
    "test": "bash -c 'source scripts/ensure-swisseph.sh && npx vitest run'",
    "start": "node dist/server/server.js"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@supabase/supabase-js": "^2.90.1",
    "@types/node": "^22.14.0",
    "bullmq": "^5.66.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "rate-limit-redis": "^4.3.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "vite": "^6.2.0",
    "vitest": "^4.0.16"
  }
}
